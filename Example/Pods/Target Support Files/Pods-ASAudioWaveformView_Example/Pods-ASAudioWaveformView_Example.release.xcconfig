ALWAYS_EMBED_SWIFT_STANDARD_LIBRARIES = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LIBRARY_SEARCH_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/ASAudioWaveformView"
OTHER_CFLAGS = $(inherited) -fmodule-map-file="${PODS_CONFIGURATION_BUILD_DIR}/ASAudioWaveformView/ASAudioWaveformView.modulemap"
OTHER_LDFLAGS = $(inherited) -ObjC -l"ASAudioWaveformView" -framework "AVFoundation"
OTHER_SWIFT_FLAGS = $(inherited) -D COCOAPODS -Xcc -fmodule-map-file="${PODS_CONFIGURATION_BUILD_DIR}/ASAudioWaveformView/ASAudioWaveformView.modulemap"
PODS_BUILD_DIR = ${BUILD_DIR}
PODS_CONFIGURATION_BUILD_DIR = ${PODS_BUILD_DIR}/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_PODFILE_DIR_PATH = ${SRCROOT}/.
PODS_ROOT = ${SRCROOT}/Pods
SWIFT_INCLUDE_PATHS = $(inherited) "${PODS_CONFIGURATION_BUILD_DIR}/ASAudioWaveformView"
USE_RECURSIVE_SCRIPT_INPUTS_IN_SCRIPT_PHASES = YES
